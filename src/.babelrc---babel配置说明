{
  ; 预设，一个预设包含多个插件，起到方便作用，不用引用多个插件
  "presets": [ 
    ; 只转换新的语法，例如const、let,不转换api
      "env",
      ; es7提案转码规则 有0 1 2 3阶段， 0 包括1 2 3里面的所有
      "stage-0",
      ; 转换react jsc语法 
      "react"
  ],
  "plugins": [
      "transform-runtime",
      ; 支持装饰器
      "transform-decorators-legacy",
      "add-module-exports"
  ]
}

